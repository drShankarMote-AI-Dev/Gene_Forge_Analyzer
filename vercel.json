{
    "version": 2,
    "framework": "vite",
    "buildCommand": "npm run build",
    "outputDirectory": "apps/client/dist",
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://backend.onrender.com/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}